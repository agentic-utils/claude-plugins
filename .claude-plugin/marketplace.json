{
  "name": "agentic-utils",
  "owner": {
    "name": "agentic-utils"
  },
  "plugins": [
    {
      "name": "ty-lsp",
      "description": "Python language server (ty) for Claude Code",
      "version": "1.0.0",
      "source": "./plugins/ty-lsp",
      "category": "development",
      "lspServers": {
        "ty": {
          "command": "ty",
          "args": ["server"],
          "extensionToLanguage": {
            ".py": "python",
            ".pyi": "python"
          }
        }
      }
    },
    {
      "name": "svelte-lsp",
      "description": "Svelte language server for Claude Code",
      "version": "1.0.0",
      "source": "./plugins/svelte-lsp",
      "category": "development",
      "lspServers": {
        "svelte": {
          "command": "svelteserver",
          "args": ["--stdio"],
          "extensionToLanguage": {
            ".svelte": "svelte"
          }
        }
      }
    }
  ]
}
